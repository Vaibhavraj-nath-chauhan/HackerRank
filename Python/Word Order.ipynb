   "source": [
    "N , dic = int(input()),dict()\n",
    "for _ in range(N):\n",
    "    word = input()\n",
    "    if word not in dic:\n",
    "        dic[word] = 1\n",
    "    else:\n",
    "        dic[word] +=1\n",
    "print(len(dic))\n",
    "print(*dic.values())"
   ]
  }
